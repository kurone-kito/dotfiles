{
  "bookmarks.label.suggestion": "suggestWhenSelected",
  "bookmarks.sideBar.expanded": true,
  "breadcrumbs.enabled": true,
  "breadcrumbs.symbolSortOrder": "name",
  "cSpell.allowCompoundWords": true,
  "cSpell.experimental.enableRegexpView": true,
  "cSpell.language": "en,eo,ja",
  "cSpell.showAutocompleteSuggestions": true,
  "cSpell.usePnP": true,
  "css.validate": false,
  "discord.detailsDebugging": "𝙱𝚁𝙰𝚅𝙾 𝘾𝙃𝘼𝙍𝙇𝙄𝙀 {current_line}",
  "discord.detailsEditing": "𝙰𝙻𝙿𝙷𝙰 𝘽𝙍𝘼𝙑𝙊 {current_line}",
  "discord.detailsIdling": "𝙰𝙻𝙿𝙷𝙰 𝘾𝙃𝘼𝙍𝙇𝙄𝙀",
  "discord.lowerDetailsDebugging": "𝙴𝙲𝙷𝙾 𝘿𝙀𝙇𝙏𝘼 {total_lines}",
  "discord.lowerDetailsEditing": "𝙲𝙷𝙰𝚁𝙻𝙸𝙴 𝘿𝙀𝙇𝙏𝘼 {total_lines}",
  "discord.lowerDetailsIdling": "𝙳𝙴𝙻𝚃𝙰 𝙀𝘾𝙃𝙊 🐱",
  "docker.truncateLongRegistryPaths": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true
  },
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.find.globalFindClipboard": true,
  "editor.fontFamily": "HackGen, 'BIZ UDGothic', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 13,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.guides.bracketPairs": true,
  "editor.hideCursorInOverviewRuler": true,
  "editor.inlineSuggest.enabled": true,
  "editor.lineNumbers": "on",
  "editor.minimap.maxColumn": 70,
  "editor.mouseWheelZoom": false,
  "editor.quickSuggestions": {
    "comments": true
  },
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "selection",
  "editor.rulers": [75, 100, 120],
  "editor.semanticHighlighting.enabled": true,
  "editor.smoothScrolling": true,
  "editor.snippetSuggestions": "bottom",
  "editor.suggest.localityBonus": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "emmet.showSuggestionsAsSnippets": true,
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.useESLintClass": true,
  "explorer.confirmDelete": false,
  "explorer.openEditors.visible": 5,
  "extensions.closeExtensionDetailsOnViewChange": true,
  "files.associations": {
    ".gitignore": "ignore"
  },
  "files.autoSave": "onFocusChange",
  "files.defaultLanguage": "markdown",
  "files.eol": "\n",
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.git": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.DS_Store": true,
    "**/desktop.ini": true,
    "**/Thumbs.db": true
  },
  "files.insertFinalNewline": true,
  "files.watcherExclude": {
    "**/.eslintcache": true,
    "**/.pnp.*": true,
    "**/.vagrant/**": true,
    "**/.yarn/cache/**": true,
    "**/.yarn/unplugged/**": true,
    "**/packages/**/.tsconfig.*.tsbuildinfo": true,
    "**/packages/**/.tsconfig.tsbuildinfo": true,
    "**/packages/**/coverage/**": true,
    "**/packages/**/dist/**": true
  },
  "git.autofetch": true,
  "git.autoStash": true,
  "git.confirmSync": false,
  "git.enableCommitSigning": true,
  "git.enableSmartCommit": true,
  "git.fetchOnPull": true,
  "git.mergeEditor": false,
  "git.promptToSaveFilesBeforeCommit": "staged",
  "git.supportCancellation": true,
  "git.untrackedChanges": "separate",
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": true
  },
  "githubPullRequests.pullBranch": "never",
  "japanese-proofreading.textlint.全角文字と半角文字の間": false,
  "japanese-proofreading.textlint.外来語カタカナ表記": false,
  "japanese-proofreading.textlint.丸かっこ（）": false,
  "japanese-proofreading.textlint.ひらく漢字": false,
  "javascript.referencesCodeLens.enabled": true,
  "javascript.suggest.completeFunctionCalls": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jest.showCoverageOnLoad": true,
  "js/ts.implicitProjectConfig.checkJs": true,
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "json.schemas": [],
  "markdown.preview.fontFamily": "-apple-system, BlinkMacSystemFont, 'BIZ UDPGothic', system-ui, 'Ubuntu', 'Droid Sans', sans-serif, 'Segoe UI Emoji'",
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "merge-conflict.diffViewPosition": "Below",
  "nasc-touchbar.addCursorBelow": false,
  "nasc-touchbar.gitSync": true,
  "nasc-touchbar.showCommands": false,
  "nasc-touchbar.showReferences": true,
  "omnisharp.monoPath": "/Library/Frameworks/Mono.framework/Versions/Current",
  "omnisharp.useModernNet": false,
  "poshProTools.checkForModuleUpdates": true,
  "poshProTools.excludeAutomaticVariables": true,
  "powermode.enabled": true,
  "powermode.enableShake": false,
  "powermode.presets": "particles",
  "powershell.codeFormatting.preset": "Allman",
  "powershell.codeFormatting.trimWhitespaceAroundPipe": true,
  "powershell.codeFormatting.useConstantStrings": true,
  "powershell.codeFormatting.useCorrectCasing": true,
  "powershell.integratedConsole.showOnStartup": false,
  "powershell.pester.useLegacyCodeLens": false,
  "powershell.pester.debugOutputVerbosity": "Minimal",
  "powershell.pester.outputVerbosity": "Minimal",
  "powershell.powerShellDefaultVersion": "PowerShell Core 6 (x64)",
  "prettier.htmlWhitespaceSensitivity": "strict",
  "problems.showCurrentInStatus": true,
  "redhat.telemetry.enabled": true,
  "scm.alwaysShowRepositories": true,
  "scm.defaultViewMode": "tree",
  "search.globalFindClipboard": true,
  "search.showLineNumbers": true,
  "security.workspace.trust.untrustedFiles": "open",
  "shellcheck.customArgs": ["-x"],
  "stylelint.enable": true,
  "tailwindcss.documentColors": true,
  "tailwindCSS.emmetCompletions": true,
  "tailwindcss.hoverColorHint": "rgb",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.enableBell": true,
  "terminal.integrated.fontFamily": "'HackGen Console NFJ', 'HackGenNerd Console', 'HackGen Console', 'BIZ UDGothic', Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.minimumContrastRatio": 1,
  "terminal.integrated.profiles.osx": {
    "bash": {
      "path": "bash",
      "icon": "terminal-bash"
    },
    "fish": {
      "path": "fish"
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    },
    "zsh": {
      "path": "zsh",
      "args": ["-l"]
    }
  },
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.windowsEnableConpty": true,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.locale": "ja",
  "typescript.referencesCodeLens.enabled": true,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.closeWhenEmpty": true,
  "window.commandCenter": true,
  "workbench.colorCustomizations": {
    "editor.selectionHighlightBackground": "#ff000030",
    "editor.selectionBackground": "#2cfafa80",
    "editor.selectionHighlightBorder": "#ff000080",
    "terminal.ansiBlack": "#4E596B",
    "terminal.ansiBlue": "#9686F5",
    "terminal.ansiBrightBlack": "#92A1B1",
    "terminal.ansiBrightBlue": "#53A5FB",
    "terminal.ansiBrightCyan": "#84D2C0",
    "terminal.ansiBrightGreen": "#AEF37D",
    "terminal.ansiBrightMagenta": "#FC5FA3",
    "terminal.ansiBrightRed": "#FC6A5D",
    "terminal.ansiBrightWhite": "#92A1B1",
    "terminal.ansiBrightYellow": "#FD8F3F",
    "terminal.ansiCyan": "#7AC8B6",
    "terminal.ansiGreen": "#75B492",
    "terminal.ansiMagenta": "#FC5FA3",
    "terminal.ansiRed": "#FC6A5D",
    "terminal.ansiWhite": "#BFBFBF",
    "terminal.ansiYellow": "#FD8F3F",
    "terminal.foreground": "#BFBFBF",
    "terminal.selectionBackground": "#314F78"
  },
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.sharedViewState": true,
  "workbench.startupEditor": "none",
  "workbench.editor.tabSizing": "shrink",
  "workbench.enableExperiments": true,
  "workbench.view.alwaysShowHeaderActions": true,
  "[bat]": {
    "editor.tabSize": 2,
    "files.encoding": "shiftjis",
    "files.eol": "\r\n"
  },
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp",
    "editor.tabSize": 4
  },
  "[javascript][javascriptreact][json][jsonc][markdown][typescript][typescriptreact][yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
