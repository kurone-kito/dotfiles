{
  "breadcrumbs.enabled": true,
  "css.lint.zeroUnits": "warning",
  "css.lint.unknownVendorSpecificProperties": "warning",
  "css.lint.universalSelector": "warning",
  "css.lint.importStatement": "warning",
  "css.lint.important": "warning",
  "css.lint.ieHack": "warning",
  "css.lint.idSelector": "warning",
  "css.lint.float": "warning",
  "css.lint.duplicateProperties": "warning",
  "css.lint.compatibleVendorPrefixes": "warning",
  "css.lint.boxModel": "warning",
  "debug.node.autoAttach": "on",
  "docker.truncateLongRegistryPaths": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.source.organizeImports": true
  },
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.find.globalFindClipboard": true,
  "editor.fontFamily": "'HackGen Console for Powerline', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.glyphMargin": false,
  "editor.gotoLocation.multiple": "gotoAndPeek",
  "editor.hideCursorInOverviewRuler": true,
  "editor.lineNumbers": "on",
  "editor.minimap.maxColumn": 70,
  "editor.mouseWheelZoom": false,
  "editor.quickSuggestions": {
    "comments": true
  },
  "editor.renderLineHighlight": "all",
  "editor.rulers": [75, 100, 120],
  "editor.smoothScrolling": true,
  "editor.snippetSuggestions": "bottom",
  "editor.suggest.localityBonus": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "emmet.showSuggestionsAsSnippets": true,
  "eslint.alwaysShowStatus": true,
  "eslint.autoFixOnSave": true,
  "eslint.provideLintTask": true,
  "eslint.validate": [
    {
      "language": "javascript",
      "autoFix": true
    },
    {
      "language": "javascriptreact",
      "autoFix": true
    },
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ],
  "explorer.confirmDelete": false,
  "explorer.openEditors.visible": 5,
  "extensions.closeExtensionDetailsOnViewChange": true,
  "files.autoSave": "onFocusChange",
  "files.defaultLanguage": "markdown",
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/desktop.ini": true,
    "**/Thumbs.db": true
  },
  "files.insertFinalNewline": true,
  "git.autofetch": true,
  "git.autoStash": true,
  "git.confirmSync": false,
  "git.enableCommitSigning": true,
  "git.enableSmartCommit": true,
  "git.fetchOnPull": true,
  "git.promptToSaveFilesBeforeCommit": true,
  "html.format.endWithNewline": true,
  "javascript.implicitProjectConfig.checkJs": true,
  "javascript.implicitProjectConfig.experimentalDecorators": true,
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.referencesCodeLens.enabled": true,
  "javascript.suggest.completeFunctionCalls": true,
  "jest.showCoverageOnLoad": true,
  "markdownlint.config": {},
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "merge-conflict.diffViewPosition": "Below",
  "nasc-touchbar.addCursorBelow": false,
  "nasc-touchbar.gitSync": true,
  "nasc-touchbar.showCommands": false,
  "nasc-touchbar.showReferences": true,
  "npm.enableScriptExplorer": true,
  "plantuml.urlFormat": "svg",
  "powermode.enabled": true,
  "powermode.enableShake": false,
  "powermode.presets": "particles",
  "prettier.htmlWhitespaceSensitivity": "strict",
  "prettier.eslintIntegration": true,
  "scss.lint.zeroUnits": "warning",
  "scss.lint.unknownVendorSpecificProperties": "warning",
  "scss.lint.universalSelector": "warning",
  "scss.lint.importStatement": "warning",
  "scss.lint.important": "warning",
  "scss.lint.ieHack": "warning",
  "scss.lint.idSelector": "warning",
  "scss.lint.float": "warning",
  "scss.lint.duplicateProperties": "warning",
  "scss.lint.compatibleVendorPrefixes": "warning",
  "scss.lint.boxModel": "warning",
  "search.globalFindClipboard": true,
  "search.showLineNumbers": true,
  "search.usePCRE2": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.enableBell": true,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.rendererType": "canvas",
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.windowsEnableConpty": true,
  "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
  "terminal.integrated.shellArgs.osx": ["--login"],
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.locale": "ja",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.referencesCodeLens.enabled": true,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.tsserver.log": "terse",
  "typescript.tsserver.trace": "messages",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.closeWhenEmpty": true,
  "window.nativeTabs": true,
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "editor.selectionHighlightBackground": "#ff000030",
    "editor.selectionBackground": "#2cfafa80",
    "editor.selectionHighlightBorder": "#ff000080"
  },
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.swipeToNavigate": true,
  "workbench.editor.tabSizing": "shrink",
  "workbench.useExperimentalGridLayout": true,
  "workbench.view.alwaysShowHeaderActions": true,
  "[bat]": {
    "editor.tabSize": 4,
    "files.encoding": "shiftjis",
    "files.eol": "\r\n"
  },
  "[csharp]": {
    "editor.tabSize": 4
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
